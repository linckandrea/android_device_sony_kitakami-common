# WiFi MAC address
genfscon sysfs /devices/platform/bcmdhd_wlan/macaddr                                u:object_r:sysfs_addrsetup:s0
genfscon sysfs /devices(/soc\.0)?/bcmdhd_wlan.83/macaddr                            u:object_r:sysfs_addrsetup:s0

genfscon sysfs /devices/soc\.0/f9923000\.spi/spi_master/spi0/spi0\.1/irq            u:object_r:sysfs_fpcd_irq:s0
genfscon sysfs /devices/soc\.0/f9923000\.spi/spi_master/spi0/spi0\.1/clk_enable     u:object_r:sysfs_fpcd_clk_enable:s0
genfscon sysfs /devices/soc\.0/f9923000\.spi/spi_master/spi0/spi0\.1/spi_prepare    u:object_r:sysfs_fpcd_spi_prepare:s0
genfscon sysfs /devices/soc\.0/f9923000\.spi/spi_master/spi0/spi0\.1/wakeup_enable  u:object_r:sysfs_fpcd_wakeup:s0
